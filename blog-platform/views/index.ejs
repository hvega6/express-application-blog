<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My Blog</title>
</head>
<body>
  <h1>Welcome to My Blog</h1>
  <% posts.forEach(post => { %>
    <div class="post">
      <h2><a href="/post/<%= post.title %>"><%= post.title %></a></h2>
      <p><%= post.content %></p>
    </div>
  <% }); %>
</body>
</html>
